<div class="page_podcasts">
  <section class="show_list" id="podcasts">
    <header class="show_list-header">
      <h2>Our Shows</h2>
    </header>
    <div class="show_list-shows">
      <%= for podcast <- @podcasts do %>
        <%= render("_podcast_summary.html", conn: @conn, podcast: podcast) %>
      <% end %>
      <%= render("_podcast_summary.html", conn: @conn, podcast: Changelog.Podcast.master) %>
    </div>
  </section>
</div>
