<div class="feed">
  <article class="news_item">
    <%= render("_item_header.html", assigns) %>
    <%= render("_item_toolbar.html", assigns) %>
    <%= if @item.story do %>
      <div class="news_item-content richtext">
        <p><%= @item.story |> md_to_html |> raw %></p>
      </div>
    <% end %>
  </article>
</div>
